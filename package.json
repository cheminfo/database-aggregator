{
  "name": "database-aggregator",
  "version": "1.0.1",
  "description": "A database aggregator",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "eslint": "eslint bin examples src test",
    "eslint-fix": "npm run eslint -- --fix",
    "test": "npm run test-only && npm run eslint",
    "test-coverage": "jest --runInBand --coverage",
    "test-only": "jest --runInBand"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test/"
    ],
    "setupFiles": [
      "./test/setup.js"
    ],
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/database-aggregator.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/database-aggregator/issues"
  },
  "homepage": "https://github.com/cheminfo/database-aggregator#readme",
  "dependencies": {
    "debug": "^3.1.0",
    "find": "^0.2.4",
    "is-running": "^2.0.0",
    "kcors": "^2.2.1",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^3.0.0",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.4.0",
    "lodash.chunk": "^4.2.0",
    "lodash.isequal": "^4.0.0",
    "mongoose": "^5.0.15",
    "pm2": "^2.10.2",
    "pm2-bridge": "^2.0.1",
    "process-scheduler": "^0.0.4"
  },
  "devDependencies": {
    "eslint": "^4.13.0",
    "eslint-config-cheminfo": "^1.9.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "jest": "^22.4.3",
    "js-yaml": "^3.11.0"
  }
}
