      "<rootDir>/test/"
    ],
    "setupFiles": [
      "./test/setup.js"
    ],
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/database-aggregator.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/database-aggregator/issues"
  },
  "homepage": "https://github.com/cheminfo/database-aggregator#readme",
  "dependencies": {
    "debug": "^3.1.0",
    "find": "^0.2.9",
    "is-running": "^2.1.0",
    "kcors": "^2.2.2",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.4.0",
    "lodash.chunk": "^4.2.0",
    "lodash.isequal": "^4.5.0",
    "mongoose": "^5.2.8",
    "pm2-bridge": "^2.0.1",
    "process-scheduler": "^0.0.4"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-cheminfo": "^1.18.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.21.0",
    "jest": "^23.5.0",
    "js-yaml": "^3.12.0"
  }
