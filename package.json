{
  "name": "database-aggregator",
  "version": "2.0.0",
  "description": "A database aggregator",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "lib",
    "lib-es6"
  ],
  "bin": {
    "database-aggregator": "./lib/bin/database-aggregator.js"
  },
  "scripts": {
    "test": "npm run test-only && npm run tslint",
    "test-coverage": "jest --runInBand --coverage",
    "test-only": "jest --runInBand",
    "clean": "rimraf lib lib-es6",
    "tsc": "npm run clean && npm run tsc-es5 && npm run tsc-es6",
    "tsc-es5": "tsc",
    "tsc-es6": "tsc --project tsconfig.es6.json",
    "tslint": "tslint --project tsconfig.base.json",
    "tslint-fix": "npm run tslint -- --fix"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "./test/setup.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/database-aggregator.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/database-aggregator/issues"
  },
  "homepage": "https://github.com/cheminfo/database-aggregator#readme",
  "dependencies": {
    "@types/mongoose": "^5.2.7",
    "debug": "^3.1.0",
    "find": "^0.2.9",
    "is-running": "^2.1.0",
    "kcors": "^2.2.2",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.4.0",
    "lodash.chunk": "^4.2.0",
    "lodash.isequal": "^4.5.0",
    "make-promises-safe": "^1.1.0",
    "mongoose": "^5.2.9",
    "pm2-bridge": "^2.0.1",
    "process-scheduler": "^0.0.4"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "jest": "^23.5.0",
    "js-yaml": "^3.12.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.1"
  }
}
